#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
    string s;
    cout << "enter the sentence";
    getline(cin,s);
    vector<bool>alpha(26,0);
    for(int i=0;i<s.size();i++)
    {
        int index=s[i]-'a';
        alpha[index]=1;
    }
    for(int i=0;i<26;i++)
    {
        if(alpha[i]==0)
        {
            return 0;
        }
    }
    return 1;
}
